@font-face {
  font-family: 'elena-web';
  src: 
    url(<%=asset_url'fonts/elena-web-bold.woff2'%>) format('woff2'),
    url(<%=asset_url'fonts/elena-web-bold.woff'%>) format('woff'),
    url(<%=asset_url'fonts/elena-web-bold.eot'%>) format('opentype');
  font-style: normal;
  font-weight: 700;
  font-display: swap;
}

@font-face {
  font-family: 'elena-web';
  src: 
    url(<%=asset_url'fonts/elena-web-medium.woff2'%>) format('woff2'),
    url(<%=asset_url'fonts/elena-web-medium.woff'%>) format('woff'),
    url(<%=asset_url'fonts/elena-web-medium.eot'%>) format('opentype');
  font-style: normal;
  font-weight: 400;
  font-display: swap;
}
