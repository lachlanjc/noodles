<% title 'Recipes' %>
<% activate_nav!(:recipes) %>

<%= render 'shared/nav' %>

<% if @recipes.any? %>
  <%= react_component 'RecipesHome',
    recipes: @recipes_json,
    groceries: @groceries_json,
    collections: @collections_json
  %>
<% else %>
  <%= react_component 'RecipesHomeBlank' %>
<% end %>

<%= render 'import' %>
